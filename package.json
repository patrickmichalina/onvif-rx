{
  "name": "onvif-rx",
  "version": "0.0.0-development",
  "description": "Interact with ONVIF cameras and devices using RXJS",
  "author": "Patrick Michalina <patrickmichalina@mac.com>",
  "main": "onvif-rx-cjs.js",
  "module": "onvif-rx-esm.js",
  "browser": "onvif-rx-umd.js",
  "types": "node.d.ts",
  "license": "MIT",
  "keywords": [
    "onvif",
    "camera",
    "video",
    "rtsp",
    "rtp",
    "rxjs",
    "node",
    "typescript",
    "udp",
    "electron",
    "universal",
    "ptz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickmichalina/onvif-rx"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node-dev src/index.ts",
    "lint": "tsc && tslint -p tsconfig.json",
    "build": "npm run rollup && npm run minify",
    "minify": "uglifyjs dist/onvif-rx-umd.js -o dist/onvif-rx-umd.min.js --source-map -c -m",
    "rollup": "rollup -c",
    "watch": "rollup -cw",
    "gen.api": "ts-node scripts/gen-api.ts"
  },
  "dependencies": {
    "js-sha1": "^0.6.0",
    "node-fetch": "^2.3.0",
    "rxjs": "^6.3.3",
    "typescript-monads": "^3.10.0",
    "uuid": "^3.3.2",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "@types/uuid": "^3.4.4",
    "@types/xmldom": "^0.1.29",
    "fs-extra": "^7.0.1",
    "rollup": "^1.1.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "semantic-release": "^15.13.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.1",
    "tslint-immutable": "^5.1.0",
    "typescript": "^3.2.2",
    "uglify-js": "^3.4.9"
  },
  "release": {
    "pkgRoot": "dist",
    "branch": "master",
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ]
    ]
  }
}
